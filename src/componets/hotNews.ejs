<%if (locals.list) {%>
    <% list.forEach(function(item) { %>
        <% if (item.type == 1) {%>    
            <a class="hn-wrapper" href="<%= item.url %>">
                <div class="imgwrapper">
                    <img src="<%= item.img %>">
                    <div class="time"><%= item.durition %></div> 
                </div>
                <p><%= item.tittle %></p>
            </a>
        <% } %>
   <% }) %>
<% } %>